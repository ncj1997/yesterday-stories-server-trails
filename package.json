  "dependencies": {
    "aws-sdk": "^2.1400.0",
    "mysql2": "^3.6.0",
    "uuid": "^9.0.0",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "serverless": "^3.34.0",
    "serverless-offline": "^13.2.0",
    "serverless-dotenv-plugin": "^4.0.2",
    "jest": "^29.7.0"
  },
  "engines": {
    "node": "18.x || 20.x"
  }
